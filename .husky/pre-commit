# Build the latest bundle
npm run build:prod

# Check if there are uncommitted changes in dist/
if git diff --name-only dist/ | grep -q .; then
  echo "❌ Error: dist/ has uncommitted changes after build."
  echo "Please stage the updated dist/ files before committing:"
  git diff --name-only dist/
  exit 1
fi

echo "✅ dist/ is up to date"
